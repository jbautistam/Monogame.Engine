    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\BauGame.Engine\BauGame.Engine.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Update="Content\Settings\textures-tiles.xml">
